  shorttitle = {Stressed or Stressed Out},
  author = {McEwen, Bruce S.},
  date = {2005-09},
  journaltitle = {Journal of Psychiatry and Neuroscience},
  shortjournal = {J Psychiatry Neurosci},
  volume = {30},
